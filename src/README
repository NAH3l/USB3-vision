Device : (usbd_device_descriptor)
	    bDeviceClass
		bDeviceSubClass
		bDeviceProtocol	
		idVendor
		idProduct	
		bNumConfigurations 

Configuration : (usbd_configuration_descriptor)
	    bConfigurationValue	= 1 		
		bNumInterfaces = 2 (Control et Streaming)	

Control Interface : (usbd_interface_descriptor)                                
		bInterfaceNumber = 0
		bInterfaceClass
		bInterfaceSubClass
		bInterfaceProtocol
		bNumEndpoints = 2 (IN and OUT)

Control Endpoint : (usbd_endpoint_descriptor_t)	
	Endpoint IN : (Host->Device)
		bEndpointAddress = 0x00 
		bmAttributes : USB_ATTRIB_BULK
		wMaxPacketSize
	Endpoint OUT : (Host->Device)
		bEndpointAddress = 0x00 
		bmAttributes : USB_ATTRIB_BULK
		wMaxPacketSize

Control Interface : (usbd_interface_descriptor)                                
		bInterfaceNumber = 1
		bInterfaceClass
		bInterfaceSubClass
		bInterfaceProtocol
		bNumEndpoints = 2 (IN and OUT)

Endpoint IN : (Host->Device)
		bEndpointAddress = 0x01 
		bmAttributes : USB_ATTRIB_BULK
		wMaxPacketSize
Endpoint OUT : (Host->Device)
		bEndpointAddress = 0x02
		bmAttributes : USB_ATTRIB_BULK
		wMaxPacketSize		